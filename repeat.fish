function repeat
set n "$argv[1]";
set m "$argv[2]";
for i in (seq $n) ; set i $i+1
echo "Starting date";date
sleep 5;
echo "Before Waiting";date
sleep 10
echo "After Waiting";date
 /usr/bin/mpv /usr/share/sounds/freedesktop/stereo/complete.oga;
 /usr/bin/mpv /usr/share/sounds/freedesktop/stereo/complete.oga;
 /usr/bin/mpv /usr/share/sounds/freedesktop/stereo/complete.oga;
eval $m
end
end
